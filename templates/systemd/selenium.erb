# <%= @prog %> Selenium server init script
[Unit]
Description=Selenium Server (<%= @prog %>)
After=network.target network-online.target

[Service]
EnvironmentFile=/etc/<%= @prog %>.conf
ExecStart=<% if @xvfb_run -%><%= @xvfb_run %> <% end -%><%= @java %> <%= @exec_command %> $SELENIUM_SERVER_ARGS
KillMode=process
User=<%= @user %>

[Install]
WantedBy=multi-user.target